{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "app.py": {
      "runtime": "python3.9"
    }
  }
}
