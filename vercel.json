{
  "buildCommand": "cd ../barbershop-frontend && npm install && npm run build",
  "outputDirectory": "../barbershop-frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "../barbershop-frontend/dist/index.html"
    }
  ]
}
